{
  "name": "scraping-comparador-precios",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node ./scripts/build-content.js && node ./scripts/copy-manifest.js",
    "watch:build": "vite build --watch",
    "watch:manifest": "nodemon --watch dist --ext js --exec \"node ./scripts/copy-manifest.js\"",
    "watch": "yarn watch:build & yarn watch:manifest",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
